{
	"author": "Gergely Laborci",
	"name": "zengular-app",
	"scripts": {
		"build": "webpack --progress --colors --mode production",
		"work": "webpack --progress --colors --watch  --mode development",
		"ggf": "node ./node_modules/zengular-build/src/zengular-ggf.js",
		"tail": "rlogtail -s rlogtail.sock"
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"last 2 version",
		"iOS >= 9"
	],
	"my-packages": {
		"andesite": {
			"src": "vendor/andesite",
			"pattern": "*",
			"dest": "/work/packages/composer/andesite"
		}
	},
	"google-fonts": "z-build-google-fonts.json",
	"z-build": "z-build.json",
	"dependencies": {
		"@fortawesome/fontawesome-pro": "^5.13.0",
		"balloon-css": "^1.0.4",
		"copy-text-to-clipboard": "^2.2.0",
		"font-awesome-filetypes": "^2.1.0",
		"markdown-it": "^12.3.2",
		"slide-anim": "^1.3.2",
		"zengular": "^3.0.23",
		"zengular-codex": "^2.0.29",
		"zengular-router": "^1.1.4",
		"zengular-ui": "^2.0.11",
		"zengular-util": "^1.0.10"
	},
	"devDependencies": {
		"webpack-cli": "^3.3.11",
		"zengular-build": "^3.1.6"
	}
}
